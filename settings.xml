<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              https://maven.apache.org/xsd/settings-1.0.0.xsd">
  <servers>
    <server>
      <id>Sandbox</id>
      <username>AzureBlueOcean</username>
      <password>...</password>
    </server>
  </servers>

  <profiles>
    <profile>
      <id>Sandbox</id>
      <repositories>
        <repository>
            <id>Localhost</id>
            <url>http://localhost:8088</url>
            <releases>
              <enabled>true</enabled>
            </releases>
            <snapshots>
              <enabled>true</enabled>
            </snapshots>
          </repository>
        <repository>
          <id>Sandbox</id>
          <url>https://pkgs.dev.azure.com/AzureBlueOcean/Sandbox/_packaging/Sandbox/maven/v1</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>Sandbox</id>
          <url>https://pkgs.dev.azure.com/AzureBlueOcean/Sandbox/_packaging/Sandbox/maven/v1</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>Sandbox</activeProfile>
  </activeProfiles>
</settings>